
DECLARE
I_EXISTS NUMBER;
BEGIN
  
  SELECT COUNT(1) INTO I_EXISTS FROM USER_SEQUENCES A WHERE A.sequence_name = 'SEQ_SERIAL_NUMBER';   

  IF I_EXISTS > 0 THEN
    
  EXECUTE IMMEDIATE 'DROP SEQUENCE SEQ_SERIAL_NUMBER';
  END IF;
  
  
   EXECUTE IMMEDIATE '
        CREATE   SEQUENCE SEQ_SERIAL_NUMBER
        INCREMENT BY 1
        START WITH 1
        MAXVALUE 999999999';


END;